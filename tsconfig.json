{
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "strictPropertyInitialization": false,
    "removeComments": true,
    "noUnusedLocals": true,
    "target": "ES5",
    "lib": ["dom", "es2019"],
    "sourceMap": true,
    "esModuleInterop": true,
    "downlevelIteration": true
  }
}
